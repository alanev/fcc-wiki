{
  "name": "fcc-wiki",
  "version": "1.0.0",
  "description": "Code for FreeCodeCamp challange 'Wikipedia Viewer'.",
  "main": "index.js",
  "scripts": {
    "styles": "postcss -u postcss-use --postcss-use.modules * -o app.css app.scss",
    "styles:watch": "npm run styles -- -w",
    "scripts": "babel app.es6 --presets es2015 -o app.js",
    "scripts:watch": "npm run scripts -- -w",
    "server": "browser-sync start --files * --server --index index.htm",
    "build": "npm run styles & npm run scripts",
    "dev": "npm-run-all --parallel server styles:watch scripts:watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "config": {
      "port": 1848
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alanev/fcc-wiki.git"
  },
  "keywords": [
    "boilerplate",
    "freecodecamp",
    "frontend"
  ],
  "author": "e@alanev.ru",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alanev/fcc-wiki/issues"
  },
  "homepage": "https://github.com/alanev/fcc-wiki#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.5",
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "cssnext": "^1.8.4",
    "npm-run-all": "^1.6.0",
    "postcss": "^5.0.19",
    "postcss-cli": "^2.5.1",
    "postcss-nested": "^1.0.0",
    "postcss-use": "^2.0.2"
  }
}
